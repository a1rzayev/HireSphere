{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=host;Port=port;Database=dbname;Username=username;Password=password"
  },
  "JwtSettings": {
    "SecretKey": "secret-key",
    "Issuer": "issuer",
    "Audience": "audience",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  },
  "AllowedHosts": "*"
}
